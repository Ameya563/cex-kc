{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["DEBUG"]
        },
        {
            "name": "debug main",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/main",
            "args": [
                "-b",
                "benchmarks/QBFEval2017/verilog/ctrl.e_1.a_3.E_134.A_48.c_.w_9.s_40.asp.v",
                "-v",
                "benchmarks/QBFEval2017/verilog/OrderFiles/ctrl.e_1.a_3.E_134.A_48.c_.w_9.s_40.asp_varstoelim.txt",
                // "-u",
                "-c",
                "2",
                "-r",
                "3",
                "-d",
                "20",
                "-t",
                "60"
            ],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "justMyCode": true,
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}