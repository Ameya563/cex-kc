{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["DEBUG"]
        },
        {
            "name": "debug main",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/main",
            "args": [
                "-b",
                "benchmarks/Disjunctive_Decomposition/verilog/bobsmcodic_all_bit_differing_from_cycle.v",
                "-v",
                "benchmarks/Disjunctive_Decomposition/verilog/OrderFiles/bobsmcodic_all_bit_differing_from_cycle_varstoelim.txt",
                "-u",
                // "-c",
                // "2",
                // "-r",
                // "3",
                // "-d",
                // "20",
                // "-ut",
                // "60"
            ],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "justMyCode": true,
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}